<%= form_with url: login_path, scope: :user do |f| %>
    <div>
        <%= f.label :email %>
        <%= f.email_field :email, required: true %>
    </div>
    <div>
        <%= f.label :password %>
        <%= f.password_field :password, required: true %>
    </div>
    <%= f.submit "Sign In" %>
<% end %>